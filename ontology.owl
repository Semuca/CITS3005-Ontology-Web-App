</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image4055">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/PVh5gZwvF5dYVEIk.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="114921">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image4056"/>
  <hasImage rdf:resource="image4057"/>
  <hasImage rdf:resource="image4058"/>
  <usesTool rdf:resource="needlenose_pliers"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the same pliers grasp the WiFi antenna cable and pull up carefully The really wide optical drive cable has a black "collar" that needs to be lifted up on each side. This will free the cable. The other two cables can just be pulled right out of their respective sockets.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image4056">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/gSiTy5JM3sNmTOgE.standard</dataUrl>
</owl:NamedIndividual>

