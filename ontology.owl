  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ybI6nAc4LQTck2Ca.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image4058">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dT3NfkCvGgQRmxVf.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="114922">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image4059"/>
  <hasImage rdf:resource="image4060"/>
  <usesTool rdf:resource="t9_torx_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the T9 security torx screw that secures the WiFi antenna. De-reoute the WiFi cable.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image4059">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/i3S1NU5akTSZAvlE.standard</dataUrl>
</owl:NamedIndividual>

