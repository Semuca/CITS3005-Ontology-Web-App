</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="59140">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2950"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unplug the wire that connects the fan to the motherboard by gently pulling it up.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image2950">
