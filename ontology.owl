</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="1450">
  <rdf:type rdf:resource="Procedure"/>
  <requiresTool rdf:resource="metal_spudger"/>
  <requiresTool rdf:resource="t10_torx_screwdriver"/>
  <requiresTool rdf:resource="t15_torx_screwdriver"/>
  <requiresTool rdf:resource="t20_torx_screwdriver"/>
