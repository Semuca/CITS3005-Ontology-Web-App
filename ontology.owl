<owl:NamedIndividual rdf:about="97875">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3810"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully remove the top body portion of the Kinect from the stand by guiding the cable through the hole on the underside of the body, then lifting the body off of the plastic stand.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3810">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/lES3DKGaWpbHdKXS.standard</dataUrl>
