<owl:NamedIndividual rdf:about="18361">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image1702"/>
  <usesTool rdf:resource="phillips_2_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 11.7 mm Phillips #2 screws connecting the power switch to the motherboard.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image1702">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WHNsH6Ddh1ZiEBUF.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="18362">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image1703"/>
  <hasImage rdf:resource="image1704"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The power switch is connected to the motherboard near the top left of the console. Carefully pull the connector from its socket on the motherboard. You may need to wiggle it out as you pull. Do not detach the connector by pulling on the wires. This could damage the wires or the socket.</actions>
