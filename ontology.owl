<owl:NamedIndividual rdf:about="209142">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image5669"/>
  <hasImage rdf:resource="image5670"/>
  <usesTool rdf:resource="ph000_phillips_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a Phillips #000 screwdriver, unscrew the single screw located on the battery cover. Remove the battery cover and set it aside. The screw does not detach from the battery cover, so do not attempt to separate them.</actions>
</owl:NamedIndividual>

