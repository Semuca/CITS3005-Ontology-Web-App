</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image618">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WXvJhQRwsipfN4gC.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image619">
