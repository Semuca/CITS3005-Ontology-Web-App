<owl:NamedIndividual rdf:about="7601">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image252"/>
  <hasImage rdf:resource="image253"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The speakers are soldered to the ribbon cable attached to the upper LCD, so they must be removed at the same time as the LCD. Lift the top LCD out of the plastic frame. Carefully thread the ribbon cable out of the hinge to avoid damaging it.</actions>
</owl:NamedIndividual>

