<owl:NamedIndividual rdf:about="6315">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image14"/>
  <hasImage rdf:resource="image15"/>
  <usesTool rdf:resource="opening_tool"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a plastic opening tool to pop out the battery, being careful not to snag the "Warranty Void" sticker in the process. When putting a new battery into the PSP Go, make sure you hold the "Warranty Sticker" so you don't put the battery on top of it.</actions>
</owl:NamedIndividual>

