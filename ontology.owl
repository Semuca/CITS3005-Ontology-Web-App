  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZuRLgWuZcPl4DSXv.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image299">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/D5BUKALLMJlem56p.standard</dataUrl>
</owl:NamedIndividual>

