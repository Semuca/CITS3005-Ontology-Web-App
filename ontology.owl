<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://ifixthat.org/onto.owl"
         xmlns="http://ifixthat.org/onto.owl#">

<owl:Ontology rdf:about="http://ifixthat.org/onto.owl"/>

<owl:ObjectProperty rdf:about="#prerequisiteOf">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Procedure"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#requiresTool">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Tool"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#guideOf">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Item"/>
        <rdf:Description rdf:about="#Part"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#subCategoryOf">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Item"/>
  <rdfs:range rdf:resource="#Item"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#partOf">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Part"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Part"/>
        <rdf:Description rdf:about="#Item"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasImage">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Tool"/>
        <rdf:Description rdf:about="#Step"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range rdf:resource="#Image"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#stepOf">
  <rdfs:domain rdf:resource="#Step"/>
  <rdfs:range rdf:resource="#Procedure"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usesTool">
  <rdfs:domain rdf:resource="#Step"/>
  <rdfs:range rdf:resource="#Tool"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#supplierUrl">
  <rdfs:domain rdf:resource="#Tool"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#number">
  <rdfs:domain rdf:resource="#Step"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#actions">
  <rdfs:domain rdf:resource="#Step"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#dataUrl">
  <rdfs:domain rdf:resource="#Image"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Procedure">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Item">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Part">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Tool">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Step">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Image">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>


</rdf:RDF>
