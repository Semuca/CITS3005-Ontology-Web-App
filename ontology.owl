  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BZfmij2wXKWpSHul.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image1914">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/1sQmTpTFijQhAe3r.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="20522">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image1915"/>
  <hasImage rdf:resource="image1916"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grab the DVD drive lens assembly by the side that the retaining pin was just removed from. Remove the lens assembly from the DVD drive housing.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image1915">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QYwQryKMu5AtfWZC.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image1916">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XHKMcSYQZEcREwG2.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="20953">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image1917"/>
  <hasImage rdf:resource="image1918"/>
  <usesTool rdf:resource="ph00_phillips_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 4.2 mm Phillips screw that secures the white plastic bracket to the lens assembly. Lift the bracket off the lens assembly. If you are going to install a new lens, make sure to transfer the white plastic bracket and screw to the new laser lens before installation.</actions>
</owl:NamedIndividual>

