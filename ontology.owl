  <hasImage rdf:resource="image3824"/>
  <hasImage rdf:resource="image3825"/>
  <usesTool rdf:resource="crosshead_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To remove arrow keys, locate the screws hidden within the key. Using 1.5 crosshead screwdriver, remove these screws and "store them in a safe place."</actions>
</owl:NamedIndividual>

