</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="55857">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2886"/>
  <hasImage rdf:resource="image2887"/>
  <usesTool rdf:resource="needlenose_pliers"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wedge a thin screwdriver underneath the LED light, and pull up so that the LED light is facing toward the ceiling. Use pliers on the pins to wiggle and loosen them until they can be removed from the motherboard.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image2886">
  <rdf:type rdf:resource="Image"/>
