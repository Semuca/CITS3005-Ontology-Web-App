</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="98337">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3819"/>
  <hasImage rdf:resource="image3820"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry open the controller.  Optionally, gently push in the thumbstick to eject the back half.  (Some buttons may fall out.) This will leave you with 2 separate pieces.  Disconnect rumble motors, using fingers on sides of connectors.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3819">
  <rdf:type rdf:resource="Image"/>
