</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image2430">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xiMPH1ANObatdomc.standard</dataUrl>
