  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2529"/>
  <hasImage rdf:resource="image2530"/>
  <hasImage rdf:resource="image2531"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your fingers to grab the two sides of the connector and gently wiggle out. Be sure not to pull on the cables.  This may pull the cables out and damage your device even more.</actions>
</owl:NamedIndividual>

