  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nintendo 3DS XL 2015 Directional Pad Replacement</rdfs:label>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="Nintendo_3DS_XL_2015_Directional_Pad">
  <rdf:type rdf:resource="Part"/>
  <partOf rdf:resource="Nintendo_3DS_XL_2015"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Directional Pad</rdfs:label>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="95436">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3711"/>
  <usesTool rdf:resource="jis_driver_set"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the JIS #000, remove four 3mm screws from the square panel. Remove the panel by lifting it upward.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3711">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/jRMQkSk2HFSXUrIi.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="95439">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3712"/>
  <usesTool rdf:resource="tweezers"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry off the gray rubber covering. Take out the directional pad using tweezers and replace.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3712">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HptmxCAEdc4LMfbC.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="42017">
  <rdf:type rdf:resource="Procedure"/>
  <requiresTool rdf:resource="phillips_screwdriver"/>
  <requiresTool rdf:resource="opening_tool"/>
  <requiresTool rdf:resource="4_allen_wrench"/>
