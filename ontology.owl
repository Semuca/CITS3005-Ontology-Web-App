  <hasImage rdf:resource="image2524"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently lift the back panel up, separating it from the front panel. Some of the buttons may fall out if they are knocked around.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image2524">
