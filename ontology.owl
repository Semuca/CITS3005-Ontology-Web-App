  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2"/>
  <hasImage rdf:resource="image3"/>
  <usesTool rdf:resource="ph00_phillips_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the four Phillips screws on the back of the PSP Go. Rotate the PSP so you can see the top of the device. Unscrew the two Phillips screws next to the volume buttons.</actions>
</owl:NamedIndividual>

