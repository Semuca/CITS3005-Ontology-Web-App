  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the six 12mm T10 Torx screws.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3730">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2uR1SKiCYGMwiIRQ.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="96840">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3731"/>
  <hasImage rdf:resource="image3732"/>
  <hasImage rdf:resource="image3733"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the top portion of the case from the rest of the body. Detach the side portions of the case from the body. Remove the fan piece from the side of the body. Unplug it from the logic board to fully remove it. When unplugging the fan, be sure to pull from the white connector at the end of the wire to avoid breaking it.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3731">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xwJsAh1KHFdUnHxd.standard</dataUrl>
</owl:NamedIndividual>

