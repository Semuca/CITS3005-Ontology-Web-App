  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6HH2LEaDBslDCarl.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="59336">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2958"/>
  <hasImage rdf:resource="image2959"/>
