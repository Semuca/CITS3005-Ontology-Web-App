</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="42113">
  <rdf:type rdf:resource="Procedure"/>
  <requiresTool rdf:resource="4_allen_wrench"/>
  <requiresTool rdf:resource="phillips_screwdriver"/>
  <requiresTool rdf:resource="opening_tool"/>
  <requiresTool rdf:resource="ph0_phillips_screwdriver"/>
  <guideOf rdf:resource="Logitech_G27_Optical_Encoder"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Logitech G27 Optical Encoder Replacement</rdfs:label>
</owl:NamedIndividual>

