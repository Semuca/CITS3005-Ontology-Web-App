<owl:NamedIndividual rdf:about="18298">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image1690"/>
  <hasImage rdf:resource="image1691"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove 4 Phillips screws from the board. Lift the main board from the bottom end and flip it over to reveal the last connector. Keep the main board fairly close to the case, making sure not to damage the last ribbon cable holding it in place.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image1690">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HXOvwVlCv1OUYCK1.standard</dataUrl>
</owl:NamedIndividual>

