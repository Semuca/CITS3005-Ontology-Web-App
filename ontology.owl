</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="10349">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image614"/>
  <hasImage rdf:resource="image615"/>
  <hasImage rdf:resource="image616"/>
  <usesTool rdf:resource="tweezers"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When you remove the motherboard from the  front case, the start and select buttons may fall out. If the buttons do not fall out, remove them with a pair of tweezers.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image614">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Igh2iHoZtljr2fLB.standard</dataUrl>
</owl:NamedIndividual>

