<owl:NamedIndividual rdf:about="6309">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image4"/>
  <hasImage rdf:resource="image5"/>
  <usesTool rdf:resource="opening_tool"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a plastic opening tool around the perimeter of the back cover to loosen it on all sides. Open the back cover by gently lifting the right side, and rotating it to the left side (as if you were opening a book). Be careful not to rotate the back cover from the bottom, as you will break the plastic screw holes on top of the device.</actions>
</owl:NamedIndividual>

