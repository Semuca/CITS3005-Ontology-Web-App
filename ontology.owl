</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="15434">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image608"/>
  <usesTool rdf:resource="ph00_phillips_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two gold Phillips #00 screws from the motherboard.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image608">
  <rdf:type rdf:resource="Image"/>
