  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nintendo Wii U Pro Controller ABXY Buttons Replacement</rdfs:label>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="Nintendo_Wii_U_Pro_Controller_ABXY_Buttons">
  <rdf:type rdf:resource="Part"/>
