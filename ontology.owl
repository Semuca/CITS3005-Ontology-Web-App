<owl:NamedIndividual rdf:about="95434">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3710"/>
  <usesTool rdf:resource="jis_driver_set"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove five 5mm screws from the game card reader panel using a JIS #000 screwdriver. The game card reader can now be removed and replaced.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3710">
  <rdf:type rdf:resource="Image"/>
