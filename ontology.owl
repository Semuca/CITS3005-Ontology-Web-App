  <hasImage rdf:resource="image273"/>
  <usesTool rdf:resource="phillips_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 6.4 mm black Phillips screws securing the optical disc drive to the bottom case. Lift and remove the optical disc drive straight up out of the bottom case.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image272">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/VDT6weYJGjTRmYVU.standard</dataUrl>
</owl:NamedIndividual>

