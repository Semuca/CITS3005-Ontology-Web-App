</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="97221">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3735"/>
  <hasImage rdf:resource="image3736"/>
  <usesTool rdf:resource="t10_torx_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 7mm T10 Torx screws (two on each side of the metal body).</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3735">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HIUdel4KPPYndksn.standard</dataUrl>
</owl:NamedIndividual>

