  <hasImage rdf:resource="image3435"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the trigger button on the top right corner by pulling upwards and outwards. Remove the right red side panel by pulling outwards.</actions>
</owl:NamedIndividual>

