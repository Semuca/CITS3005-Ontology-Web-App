  <hasImage rdf:resource="image3792"/>
  <usesTool rdf:resource="phillips_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the device over and four 15.6mm screws with a Phillips #1 screwdriver. Pull the lower clamp up and out of the shifter assembly.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3792">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6YETxgRNfNnCv2tZ.standard</dataUrl>
</owl:NamedIndividual>

