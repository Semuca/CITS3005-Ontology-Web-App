  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZO1NNC2yuIJGtXXR.standard</dataUrl>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="43224">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2510"/>
  <hasImage rdf:resource="image2511"/>
  <hasImage rdf:resource="image2512"/>
  <usesTool rdf:resource="tr10_torx_security_screwdriver"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screws and cut or peel the security sticker on the side of the DVD drive (also voids the warranty).  Lift off the bottom and top metal case.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image2510">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YPxUGASxaPXXIuUo.standard</dataUrl>
</owl:NamedIndividual>

