<owl:NamedIndividual rdf:about="17142">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image1527"/>
  <hasImage rdf:resource="image1528"/>
  <hasImage rdf:resource="image1529"/>
  <usesTool rdf:resource="spudger"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the flat end of a spudger, remove the adhesive securing the volume control switch ribbon cable to the back of the battery compartment. Using the flat of a spudger, pry the volume control switch straight up out of the lower case. Remove the volume control switch from the DSi XL.</actions>
</owl:NamedIndividual>

