  <hasImage rdf:resource="image3793"/>
  <hasImage rdf:resource="image3794"/>
  <hasImage rdf:resource="image3795"/>
  <usesTool rdf:resource="10_mm_socket"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 10mmm hex nuts with a 10mm socket and a ratchet. Next, unscrew the clamps by holding the lower portion of the shifter and unscrewing the top part. Also remove the nut on the screw if it wasn't removed with the lower clamp.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3793">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LUKfXHEEVLteJlpG.standard</dataUrl>
</owl:NamedIndividual>

