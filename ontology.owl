  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image825"/>
  <hasImage rdf:resource="image826"/>
  <usesTool rdf:resource="spudger"/>
  <usesTool rdf:resource="metal_spudger"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert a metal spudger through the slit near the rear left corner of the bottom bezel. Rotate the spudger away from the device while lifting the rear bezel up with the other hand. You may hear a "pop" signifying the clip has cleared its catch.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image825">
