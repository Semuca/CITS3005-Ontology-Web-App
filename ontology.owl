  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image250"/>
  <usesTool rdf:resource="spudger"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On the reverse side of the casing, insert the spudger onto the surface of the sliding shaft just left of the LED light channels. Push the sliding shaft completely through the hinge until it stops and the hinge is released.</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image250">
  <rdf:type rdf:resource="Image"/>
