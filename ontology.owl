  <subProcedureOf rdf:resource="42119"/>
  <subProcedureOf rdf:resource="42311"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disassembling Logitech G27 Steering Wheel</rdfs:label>
</owl:NamedIndividual>

