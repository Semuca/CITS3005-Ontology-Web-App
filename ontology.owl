<owl:NamedIndividual rdf:about="67652">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image3242"/>
  <hasImage rdf:resource="image3243"/>
  <hasImage rdf:resource="image3244"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a "Helping Hands" or similar tool to hold the circuit board steady for the solder work. These are the solder connections that will need to be desoldered. Since the board is upside down, left will become right. Use a desoldering wick and flux to melt and remove the solder</actions>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="image3242">
  <rdf:type rdf:resource="Image"/>
  <dataUrl rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/vTTZkwFEOxjjXgtm.standard</dataUrl>
