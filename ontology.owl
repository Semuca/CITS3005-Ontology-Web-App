<owl:NamedIndividual rdf:about="45421">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2190"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is a simple procedure for the Hitachi and Samsung/Toshiba Drives (you only need to disconnect two ribbon cables and then reattach them) in the BenQ drive you need to de-solder and resolder 9 wires too.</actions>
</owl:NamedIndividual>

