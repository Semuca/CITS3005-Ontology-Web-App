<owl:NamedIndividual rdf:about="26474">
  <rdf:type rdf:resource="Step"/>
  <hasImage rdf:resource="image2139"/>
  <hasImage rdf:resource="image2140"/>
  <actions rdf:datatype="http://www.w3.org/2001/XMLSchema#string">De-route the Wi-Fi antenna cable from the bottom of the upper case. This step is not necessary if you are only replacing the front panel; however, the replacement of the upper screen, speakers, cameras, and Wi-Fi antenna require it.</actions>
</owl:NamedIndividual>

